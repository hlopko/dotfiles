#!/bin/bash

# if git rev-parse --git-dir > /dev/null 2>&1; then
#   git ls-files . -co --exclude-standard
# else
  find * -type f \
    -and -not -path 'tmp*' \
    -and -not -path 'out*' \
    -and -not -path '*.o' \
    -and -not -path '*.so' \
    -and -not -path 'doc*' \
    -and -not -path 'cache*' \
    -and -not -path '.vendor*' \
    -and -not -path 'target*' \
    -and -not -path '.bundle*' \
    -and -not -path 'bundle*' \
    -and -not -path 'native_client*' \
    -and -not -path 'public/uploads*' \
    -and -not -path '*.ttf' \
    -and -not -path '*.eot' \
    -and -not -path '*.woff' \
    -and -not -path '*.svg' \
    -and -not -path '*.gif' \
    -and -not -path '*.jpg' \
    -and -not -path '*.png' \
    -and -not -path 'log*' \
    -and -not -path '*.keep' \
    -and -not -path 'java/target*' \
    -and -not -path 'java/doc*'
# fi
