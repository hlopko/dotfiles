#!/bin/bash

find * -type f \
  -and -not -path 'tmp*' \
  -and -not -path '*.o' \
  -and -not -path '*.so' \
  -and -not -path 'doc*' \
  -and -not -path 'cache*' \
  -and -not -path 'vendor*' \
  -and -not -path '.vendor*' \
  -and -not -path 'target*' \
  -and -not -path '.bundle*' \
  -and -not -path 'bundle*' \
  -and -not -path 'log*' \
  -and -not -path '*.keep' \
  -and -not -path 'java/target*' \
  -and -not -path 'java/doc*'
